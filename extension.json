  "bugs": "https://github.com/MohamedElashri/SublimeMerge.novaextension/issues",
  "license": "MIT",
  "main": "main.js",
  "entitlements": {
    "process": true,
    "filesystem": "readwrite"
  },
  "commands": {
    "extensions": [
      {
        "title": "Open Workspace in Sublime Merge",
        "command": "submerge.submerge"
      }
    ]
  }
}
